Create Procedure [dbo].[GetLine]
 AS
BEGIN

SELECT
s.Line_ID,
s.Plant_ID,
p.Plant_Code,
s.Dept_ID,
d.Dept_Name,
s.Line_Name,
C.Sup_Code,
s.Active_Status
From Mst_Line s
    INNER JOIN 
        Mst_Plant p 
    ON 
        s.Plant_ID = p.Plant_ID
		INNER JOIN 
        Mst_Department d
    ON 
        s.Dept_ID = d.Dept_ID
		INNER JOIN 
        Mst_Module M
    ON 
        s.Module_ID = M.Module_ID
		INNER JOIN 
        Mst_SupvCode C
    ON 
        s.Supv_ID = C.Supv_ID


    END
