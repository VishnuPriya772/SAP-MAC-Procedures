-- T20: Get all Supervisor records
SELECT 
    Supv_ID,
    Plant_ID,
    Sup_Code,
    Sup_Name,
    Active_Status,
    Created_By,
    Created_On,
    Modified_By,
    Modified_On
FROM Mst_SupvCode;
-- T21: Get all Module records
SELECT 
    Module_ID,
    Plant_ID,
    Dept_ID,
    Module_Name,
    Created_By,
    Created_On,
    Modified_By,
    Modified_On
FROM Mst_Module;
-- T22: Get all Line records
SELECT 
    Line_ID,
    Plant_ID,
    Dept_ID,
    Module_ID,
    Line_Name,
    Supv_ID,
    Created_By,
    Created_On,
    Modified_By,
    Modified_On
FROM Mst_Line;

