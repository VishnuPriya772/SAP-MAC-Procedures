ALTER PROCEDURE [dbo].[Insert_Mst_StorageLocation]  
     @Plant_Code INT, 
	 @Supv_ID INT,
    @Storage_Code INT,  
    @SLoc_Name VARCHAR(100),  
   
    @Active_Status BIT,  
    @UserID VARCHAR(50)  
     
      
AS  
BEGIN  
    INSERT INTO Mst_Storage_Location(  
    Plant_ID,Supv_ID, Storage_Code, SLoc_Name, Active_Status, Created_By, Created_On  
)  
VALUES (  
    @Plant_Code,@Supv_ID, @Storage_Code, @SLoc_Name, @Active_Status, @UserID, GETDATE()  
);  
  
  
  
END  
